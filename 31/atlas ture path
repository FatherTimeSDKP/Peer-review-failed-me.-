F_VFE_NET = np.array([5.0, -10.0, 0.0])      # Sideways (5N) and Deceleration (-10N)

Predicted_Path = predict_future_path_sdvr(Initial_P, Initial_V, F_VFE_NET, np.array([0,0,0]), steps=1000)

